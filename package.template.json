{
  "private": true,
  "name": "helloworld",
  "version": "0.1.0",
  "engines": {
    "node": "^8.0.0 || ^10.0.0",
    "npm": "^5.0.0 || ^6.0.0"
  },
  "scripts": {
    "serve": "react-native start",
    "android": "react-native run-android --no-packager --appId com.helloworld.package",
    "ios": "react-native run-ios --no-packager",
    "android-release": "react-native run-android --variant=release",
    "ios-release": "react-native run-ios --configuration Release",
    "test": "jest",
    "lint": "eslint ./",
    "format": "prettier-eslint --write '**/.*.{yml,json,js}' '**/*.{md,yml,json,js,css,scss}'",
    "postinstall": "./scripts/postinstall.sh"
  },
  "dependencies": {
    "@react-native-community/async-storage": "~1.5.1",
    "helloworld-lib": "file:lib/helloworld-lib.tgz",
    "native-base": "~2.12.1",
    "prop-types": "~15.7.2",
    "react": "16.8.6",
    "react-native": "0.60.3",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-splash-screen": "~3.2.0",
    "react-navigation": "~3.11.1",
    "react-redux": "^7.1.0",
    "redux": "~4.0.4",
    "redux-localstorage": "~1.0.0-rc5",
    "redux-logger": "~3.0.6",
    "redux-thunk": "~2.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.5.4",
    "@babel/runtime": "~7.5.4",
    "babel-eslint": "~10.0.2",
    "babel-jest": "~24.8.0",
    "babel-preset-react-native": "^5",
    "eslint": "~5.16.0",
    "eslint-config-airbnb": "~17.1.1",
    "eslint-plugin-import": "~2.18.0",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-react": "~7.14.2",
    "jest": "~24.8.0",
    "jest-react-native": "~18.0.0",
    "metro-react-native-babel-preset": "0.55.0",
    "prettier-eslint-cli": "~5.0.0",
    "react-test-renderer": "16.8.6"
  }
}
